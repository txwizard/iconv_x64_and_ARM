        library directory of this repository. Unless the configuration type is
        DynamicLibrary, this target is skipped.

        Since reference libraries created by Debug and Release builds are
        identical, all but static build configurations can run this target.
    -->

    <Copy SourceFiles="@(LinkLibsOnly)"
          DestinationFolder="$(ProjectDir)..\LIB"
          OverwriteReadOnlyFiles="true"
          SkipUnchangedFiles="true" />
  </Target>

  <Target Name="AfterBuild7"
          AfterTargets="AfterBuild"
          Condition="'$(ConfigurationType)'=='DynamicLibrary' AND '$(Configuration)'=='Release'">

